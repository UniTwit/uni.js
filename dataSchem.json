{

	"twitter" : {
		"consumer_key": {
			"regex" : "^[0-9a-zA-Z]{18,25}$"
		},
		"consumer_secret":{
			"regex" : "^[0-9a-zA-Z]{38,45}$"
		},
		"access_token": {
			"regex" : "^[0-9]{8}-[0-9a-zA-Z]{38,45}$"
		},
		"access_token_secret":{
			"regex" : "^[0-9a-zA-Z]{38,45}$"
		}
	},

	"port" : {
		"dataType":"int",
		"min":10,
		"max":65535
	}

}